}
void cargar(char matriz[fil][col]){
	for(int i=0;i<19;i++){
		for(int j=0;j<61;j++){
			matriz[i][j]=32;
		}
	}
		
}

void mostrar(char matriz[fil][col]){

	for(int i=0;i<19;i++){
		for(int j=0;j<61;j++){
			cout<<matriz[i][j];
		}cout<<endl;
	}

	

}
void cargars(char matriz[fil][col]){
int x1, y1, x2, y2;
	x1=2;
	y1=2;
	x2=2;
	y2=7;
	for(int j=y1;j<=y2;j++){
			matriz[x1][j]=196;
		}
	matriz[2][2]=218;
	x1=3;
	y1=2;
	x2=5;
	y2=2;
	
	for(int j=x1;j<=x2;j++){
			matriz[j][y1]=179;
		}
	matriz[5][2]=192;

	x1=5;
	y1=3;
	x2=5;
	y2=7;
	for(int j=y1;j<=y2;j++){
			matriz[x1][j]=196;
		}
	matriz[5][7]=191;
	x1=6;
	y1=7;
	x2=8;
	y2=7;
	
	for(int j=x1;j<=x2;j++){
			matriz[j][y1]=179;
		}
	matriz[8][7]=217;

	x1=8;
	y1=2;
	x2=8;
	y2=6;
	for(int j=y1;j<=y2;j++){
			matriz[x1][j]=196;
		}
	
}
