			matriz[j][y1]=179;
		}
	x1=7;
	y1=10;
	x2=7;
	y2=14;
	for(int j=y1;j<=y2;j++){
			matriz[x1][j]=196;
		}
	x1=5;
	y1=9;
	x2=8;
	y2=9;
	
	for(int j=x1;j<=x2;j++){
			matriz[j][y1]=179;
		}
	

