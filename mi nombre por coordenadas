void main()
{	
	char matriz[fil][col];
	
	cargar(matriz);
	
	cargars(matriz);
	cargare( matriz);
	cargarb( matriz);
	cargara(matriz);
	cargarsmini(matriz);
	cargart(matriz);
	cargari(matriz);
	cargara2( matriz);
	cargarn( matriz);
	mostrar(matriz);
